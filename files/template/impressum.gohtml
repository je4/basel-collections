                    <rect
                            id="rect12"
                            height="67.9"
                            width="8.6"
                            class="st1"
                            transform="matrix(-1 -1.224647e-16 1.224647e-16 -1 132.1872 452.5284)"
                            y="192.3"
                            x="61.8" />
                    <rect
                            id="rect14"
                            height="67.9"
                            width="8.6"
                            class="st1"
