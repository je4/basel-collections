{{ $data := . }}<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Basel Collections - Impressum</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link href="../static/css/basel-collections.css" rel="stylesheet">

    <style>
        @media (max-width: 768px) {
            #title-box {
                grid-column: 1 / span 8;
            }

            #title-small {
                grid-column: 1 / span 3;
                grid-row: 2 / span 2;
            }

            #pinkbox {
                visibility: hidden;
            }

            #content {
              grid-row: 6 / span 1;
            }


            #news-black {
                visibility: hidden;
            }
            #content-small-impressum {
                grid-column: 1 / span 8;
                grid-row: 7 / span 1;
            {{ range $tag, $val := .ImpressumSmall.Scheme }}
            {{ $tag }}: {{ $val }};{{ end }}
            }
            #content-large-impressum {
                position: absolute;
                top: 0;
                left: 0;
                visibility: hidden;
                height: 0px;
                width: 0px;
            }
        }
        @media (min-width: 769px) {
            #title-large {
                grid-column: 1 / span 4;
                grid-row: 2 / span 2;
            }

            #pinkbox {
                grid-column: 10 / span 3;
                grid-row: 2 / span 2;
                background-color: #EC339B; // deeppink
            }

            #content {
                grid-row: 5;
                grid-column: 3 / span 10;
            }

            #news-black {
                grid-column: 5 / span 6;
                grid-row: 1 / span 1;
            }
            #content-large-impressum {
                grid-column: {{ .ImpressumLarge.Left }} / span {{ .ImpressumLarge.Cols }};
                grid-row: 6 / span {{ .ImpressumLarge.Rows }};
            {{ range $tag, $val := .ImpressumLarge.Scheme }}
            {{ $tag }}: {{ $val }};{{ end }}
            }
            #content-small-impressum {
                position: absolute;
                top: 0;
                left: 0;
                visibility: hidden;
                height: 0px;
                width: 0px;
            }
        }
    </style>

    <script>
        window.onload = function() {
            window.optimizeFontsize();
            window.addEventListener('resize', window.optimizeFontsize);
        }
    </script>
</head>
<body>
