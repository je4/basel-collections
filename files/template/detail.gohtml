{{ $data := . }}<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>Basel Collections</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link href="../static/css/basel-collections.css" rel="stylesheet">

    <style>


        @media (max-width: 768px) {
            #title-small {
                grid-column: 1 / span {{ .BoxSmall.Cols }};
                grid-row: 2 / span {{ .BoxSmall.Rows }};
            {{ range $tag, $val := .BoxSmall.Scheme }}
                {{ $tag }}: {{ $val }};{{ end }}
            }
            #title-large {
                position: absolute;
                top: 0;
                left: 0;
                visibility: hidden;
                height: 0px;
                width: 0px;
            }

            #content-small-impressum {
                grid-column: {{ .ImpressumSmall.Left }} / span {{ .ImpressumSmall.Cols }};
                grid-row: {{ .ImpressumSmall.Top }} / span {{ .ImpressumSmall.Rows }};
            {{ range $tag, $val := .ImpressumSmall.Scheme }}
                {{ $tag }}: {{ $val }};{{ end }}
            }
            #content-large-impressum {
                position: absolute;
                top: 0;
                left: 0;
                visibility: hidden;
                height: 0px;
                width: 0px;
            }
        }



        @media (min-width: 769px) {
            #title-large {
                grid-column: 1 / span {{ .BoxLarge.Cols }};
                grid-row: 2 / span {{ .BoxLarge.Rows }};
            {{ range $tag, $val := .BoxLarge.Scheme }}
            {{ $tag }}: {{ $val }};{{ end }}
            }
            #title-small {
                position: absolute;
                top: 0;
                left: 0;
                visibility: hidden;
                height: 0px;
                width: 0px;
            }
            
            #content-large-impressum {
                grid-column: {{ .ImpressumLarge.Left }} / span {{ .ImpressumLarge.Cols }};
                grid-row: {{ .ImpressumLarge.Top }} / span {{ .ImpressumLarge.Rows }};
            {{ range $tag, $val := .ImpressumLarge.Scheme }}
            {{ $tag }}: {{ $val }};{{ end }}
            }

            #content-small-impressum {
                position: absolute;
                top: 0;
                left: 0;
                visibility: hidden;
                height: 0px;
                width: 0px;
            }
        }
    </style>

    <script>
        window.onload = function() {
            window.optimizeFontsize();
            window.addEventListener('resize', window.optimizeFontsize);
        }
    </script>
</head>
