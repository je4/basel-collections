        <div id="title-large" class="type-{{ .BoxLarge.Type }}">
            <div class="text pos-{{ index .BoxLarge.Scheme "vertical-align" }}">
                {{ .Collection.Title }}
            </div>
        </div>
        <div id="title-small" class="type-{{ .BoxLarge.Type }}">
            <div class="text pos-{{ index .BoxLarge.Scheme "vertical-align" }}">
                {{ .Collection.Title }}
            </div>
        </div>
        <div id="title-box">
            {{ .Collection.Title }}<br />
            <a style="text-decoration: inherit; color: inherit;" href="{{ .Collection.Url }}">Collection Website<svg
                        id="Ebene_1"
                        data-name="Ebene 1"
                        xmlns="http://www.w3.org/2000/svg"
